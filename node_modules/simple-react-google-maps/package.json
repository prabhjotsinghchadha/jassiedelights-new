{
  "name": "simple-react-google-maps",
  "version": "1.1.2",
  "description": "Quick and easy Google Maps package for React.",
  "main": "lib/index.js",
  "scripts": {
    "dev": "concurrently \"npm run lib:watch\" \"npm run docs\"",
    "lib": "babel src/lib -d lib --copy-files",
    "lib:watch": "babel src/lib -w -d lib --copy-files",
    "docs": "webpack-dev-server --mode development",
    "docs:prod": "webpack --mode production"
  },
  "keywords": [
    "react",
    "google",
    "maps",
    "map",
    "simple",
    "google maps"
  ],
  "license": "MIT",
  "peerDependencies": {
    "react": "^15.3.0 || ^16.2.0",
    "react-dom": "^15.3.0 || ^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.46",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0-beta.46",
    "babel-loader": "^8.0.0-beta.0",
    "concurrently": "^3.5.1",
    "css-loader": "^2.1.1",
    "eslint": "^4.19.1",
    "eslint-plugin-react": "^7.7.0",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "author": "mypwhas9letters",
  "homepage": "https://github.com/mypwhas9letters/simple-react-google-maps#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mypwhas9letters/simple-react-google-maps.git"
  },
  "dependencies": {
    "prop-types": "^15.6.1",
    "scriptjs": "^2.5.8"
  }
}
